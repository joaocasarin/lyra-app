{
    "name": "lyra-app",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:check": "eslint --ext .js,.jsx,.ts,.tsx,.json --ignore-path .eslintignore .",
        "lint:fix": "eslint *.{ts,tsx,js,jsx,json} --fix",
        "tsc:check": "tsc --noEmit",
        "prettier:check": "prettier --check *.{ts,tsx,js,jsx,json}",
        "prettier:fix": "prettier --config .prettierrc.js *.{ts,tsx,js,jsx,json} --write",
        "prepare": "husky install"
    },
    "dependencies": {
        "@emotion/react": "11.8.1",
        "@emotion/styled": "11.8.1",
        "@mui/icons-material": "^5.4.2",
        "@mui/material": "5.4.3",
        "date-fns": "2.28.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "6.2.1",
        "uuid": "8.3.2",
        "zod": "^3.12.0"
    },
    "devDependencies": {
        "@babel/core": "7.17.5",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@types/uuid": "8.3.4",
        "@typescript-eslint/eslint-plugin": "5.12.1",
        "@typescript-eslint/parser": "5.12.1",
        "@vitejs/plugin-react": "1.2.0",
        "eslint": "8.9.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.4.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "husky": "7.0.4",
        "lint-staged": "12.3.4",
        "prettier": "2.5.1",
        "typescript": "4.5.5",
        "vite": "2.8.4"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json}": "eslint *.{ts,tsx,js,jsx,json} --fix"
    }
}
